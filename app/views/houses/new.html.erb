<h1>New House Form</h1>
<%= form_for @house do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :source %>
  <%= f.text_field :source %>
  <%= f.label :author %>
  <%= f.text_field :author %>
  <%= f.label :motto %>
  <%= f.text_field :motto %>
  <%= f.submit "Add House" %>
<% end %>